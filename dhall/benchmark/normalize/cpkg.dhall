let concatMapSep =
        λ(_ : Text)
      → λ(_ : Type)
      → λ(_ : _ → Text)
      → λ(_ : List _@1)
      → merge
          { Empty = "", NonEmpty = λ(_ : Text) → _ }
          ( List/fold
              _@2
              _
              < Empty | NonEmpty : Text >
              (   λ(_ : _@2)
                → λ(_ : < Empty | NonEmpty : Text >)
                → merge
                    { Empty = < Empty | NonEmpty : Text >.NonEmpty (_@3 _@1)
                    , NonEmpty =
                          λ(_ : Text)
                        → < Empty | NonEmpty : Text >.NonEmpty
                            "${_@4 _@2}${_@6}${_}"
                    }
                    _
              )
              < Empty | NonEmpty : Text >.Empty
          )

let concat =
        λ(_ : List Text)
      → List/fold Text _ Text (λ(_ : Text) → λ(_ : Text) → "${_@1}${_}") ""

let concatMapText =
        λ(_ : Type)
      → λ(_ : _ → Text)
      → λ(_ : List _@1)
      → List/fold _@2 _ Text (λ(_ : _@2) → λ(_ : Text) → "${_@3 _@1}${_}") ""

