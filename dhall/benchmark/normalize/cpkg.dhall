                , Android = "android64-aarch64"
                , NoOs = "gcc"
                }
                os

      in    λ(v : List Natural)
          →   prelude.simplePackage { name = "openssl", version = v }
            ⫽ { pkgUrl =
                  "https://www.openssl.org/source/openssl-${prelude.showVersion
                                                              v}d.tar.gz"
