{-
Returns `True` if the `List` is empty and `False` otherwise

Examples:

```
./null Integer ([ 0, 1, 2 ] : List Integer) = False

./null Integer ([] : List Integer) = True
```
-}
    let null
        : ∀(a : Type) → List a → Bool
        = λ(a : Type) → λ(xs : List a) → Natural/isZero (List/length a xs)

in  null
